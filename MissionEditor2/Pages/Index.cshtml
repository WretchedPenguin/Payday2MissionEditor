@page
@using MissionEditor2.Pages.Communicators
@using System.Runtime.CompilerServices
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<component type="typeof(ElementCommunicator)" render-mode="Server"/>
<component type="typeof(CommandCommunicator)" render-mode="Server"/>
<div id="app"></div>


@section Scripts{
    <script src="~/app/js/chunk-vendors.js"></script>
    <script src="~/app/js/index.js"></script>
    <script>
            Blazor.defaultReconnectionHandler._reconnectCallback = function (d) {
                document.location.reload();
            }
    </script>
}